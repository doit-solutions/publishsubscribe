<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <Company>do IT Solutions AB</Company>
    <PackageId>DoIt.PublishSubscribe</PackageId>
    <Title>do IT Publish/Subscribe</Title>
    <Description>Strongly typed publish/subscribe utility levaraging .NET Core's DI framework for subscribe/unsubscribe.</Description>
    <Authors>doit-solutions</Authors>
